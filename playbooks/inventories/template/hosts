[server]
# Name for the host, if you want to change this name, you have to rename the file
# inventories/<name>/hosts_vars/cauldron_host.yml and the directory files/cauldron_host
cauldron_host


[server:vars]
# Type of connection: ssh or local
ansible_connection=local

# Location of the host: localhost, IP or domain:
ansible_host=localhost

