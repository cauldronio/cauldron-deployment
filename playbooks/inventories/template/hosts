[webserver]
# Name for the webserver host, if you want to change this name, you have to
# rename the directory files/cauldron_host
cauldron_host

[elasticsearch]
# Name for the elasticsearch host, if you want to change this name, you have
# to rename the directory files/cauldron_host
cauldron_host

[webserver:vars]
# Location of the host: localhost, IP or domain
ansible_host=localhost

[elasticsearch:vars]
# Location of the host: localhost, IP or domain
ansible_host=localhost

[all:vars]
# Type of connection: ssh or local
ansible_connection=local

# User used for the remote location
# ansible_ssh_user=

ansible_python_interpreter=/usr/bin/python3
